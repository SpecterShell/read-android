<resources xmlns:android="http://schemas.android.com/apk/res/android">
    
    <style name="CustomDialog" parent="@android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item> 
        <item name="android:windowBackground">@color/dialog_bg</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
    </style>
    

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Holo.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="list_view_style_no_divider">
        <item name="android:cacheColorHint">#00000000</item>
        <item name="android:drawSelectorOnTop">false</item>
        <item name="android:listSelector">@drawable/item_selected_bg</item>
        <item name="android:background">@color/timeline_bg</item>
        <item name="android:divider">@color/hariline</item>
        <item name="android:dividerHeight">0dp</item>
    </style>

    <style name="wrap_height_match_width">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="wrap_height_wrap_content">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="max_7line_text" parent="wrap_height_match_width">
        <item name="android:maxLines">@integer/timeline_max_4line</item>
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">false</item>
    </style>

    <style name="timeline_user_name">
        <item name="android:layout_gravity">left</item>
        <item name="android:textColor">@color/text_main</item>
        <item name="android:textSize">@dimen/stSize16</item>
    </style>

    <style name="timeline_update_time" parent="wrap_height_match_width">
        <item name="android:layout_gravity">left</item>
        <item name="android:textColor">@color/text_sub</item>
        <item name="android:textSize">@dimen/stSize12</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="timeline_book_name">
        <item name="android:textColor">@color/timeline_book_title</item>
        <item name="android:textSize">@dimen/user_follow_number</item>
    </style>

    <style name="timeline_title" parent="timeline_book_name">
        <item name="android:textColor">@color/dark_grey_hl</item>
    </style>

    <style name="timeline_main_content">
        <item name="android:textColor">@color/community_color_title</item>
        <item name="android:textSize">@dimen/stSize14</item>
    </style>

    <style name="timeline_quote_content">
      <!--   <item name="android:textColor">@color/pie_chart_dark_gray</item> -->
        <item name="android:textColor">@color/community_color_title</item>
        <item name="android:textSize">@dimen/stSize14</item>
    </style>

    <style name="timeline_recommand" parent="wrap_height_match_width">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">#b8b8b826</item>
        <item name="android:textSize">13dp</item>
    </style>

    <style name="timeline_comment">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">@color/tab_textcolor</item>
        <item name="android:textSize">13dp</item>
    </style>

    <style name="view_pager_tab_bar_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/tab_button_bg</item>
        <item name="android:textColor">@color/tab_button_textcolor</item>
        <item name="android:textSize">12dp</item>
    </style>

    <style name="user_text">
        <item name="android:textColor">@color/tab_textcolor</item>
        <item name="android:textSize">@dimen/user_follow_number</item>
    </style>

    <style name="user_detail_row">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">@dimen/user_info</item>
        <item name="android:paddingLeft">@dimen/user_detail</item>
        <item name="android:paddingRight">@dimen/user_detail</item>
    </style>

    <style name="user_detail_text" parent="@style/user_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>

    <style name="user_detail_arrow" parent="@style/user_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:drawableRight">@drawable/icon_arrow</item>
    </style>

    <style name="user_more_value" parent="@style/user_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">@dimen/text_space</item>
        <item name="android:textColor">@color/dark_grey_hl</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>

    <style name="user_book_name">
        <item name="android:textColor">@color/dark_grey_hl</item>
        <item name="android:textSize">@dimen/user_follow_number</item>
    </style>

    <style name="user_book_author">
        <item name="android:textColor">@color/tab_textcolor</item>
        <item name="android:textSize">@dimen/user_more</item>
    </style>

    <style name="user_follow_button" parent="wrap_height_wrap_content">
        <item name="android:background">@drawable/user_follow_button</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textOff">@string/user_follow_not</item>
        <item name="android:textOn">@string/user_follow_done</item>
        <item name="android:textSize">14dp</item>
    </style>

    <style name="chat">
        <item name="android:textColor">@color/text_main</item>
        <item name="android:textSize">@dimen/stSize16</item>
    </style>

    <style name="notifity_text">
        <item name="android:textColor">@color/text_main</item>
        <item name="android:textSize">@dimen/tSize16</item>
        <item name="android:maxLines">3</item>
        <item name="android:lineSpacingMultiplier">1.2</item>
    </style>

    <style name="list_view_style_no_focus">
        <item name="android:cacheColorHint">#00000000</item>
        <item name="android:drawSelectorOnTop">false</item>
        <item name="android:listSelector">#00000000</item>
        <item name="android:background">#00000000</item>
        <item name="android:divider">#00000000</item>
        <item name="android:dividerHeight">0px</item>
    </style>

    <style name="anim_menu_bottombar">
        <item name="android:windowEnterAnimation">@anim/menu_bottombar_in</item>
        <item name="android:windowExitAnimation">@anim/menu_bottombar_out</item>
    </style>

    <style name="custom_ratingbar_style" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/custom_ratingbar_progress</item>
        <item name="android:minHeight">14dip</item>
        <item name="android:maxHeight">20dip</item>
        <item name="android:stepSize">0.5</item>
        <item name="android:isIndicator">true</item>
    </style>

    <style name="custom_ratingbar_style_big" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/custom_ratingbar_progress_big</item>
        <item name="android:minHeight">24dip</item>
        <item name="android:maxHeight">32dip</item>
        <item name="android:stepSize">0.5</item>
    </style>

    <style name="fullscreen_transparent">
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Translucent</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>



　　</style>

